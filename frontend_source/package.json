  "build": {
    "appId": "com.icaffe.kiosk",
    "productName": "icaffeos",
    "directories": {
      "output": "release",
      "buildResources": "electron-build-resources"
    },
    "files": [
      "build/**/*",
      "dist-electron/**/*",
      "package.json",
      "backend_server.js",
      "sms-server.js",
      "backend/**/*",
      ".env"
    ],
    "mac": {
      "target": "dmg",
      "icon": "electron-build-resources/icon.png",
      "extraResources": [
        {
          "from": "binaries/mac",
          "to": "binaries",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "dmg": {
      "icon": "electron-build-resources/icon.png",
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility",
      "extraResources": [
        {
          "from": "binaries/linux",
          "to": "binaries",
          "filter": [
            "**/*"
          ]
        }
      ]
    }
  }
}
